pipeline {
 agent any
   stages {
		stage ("Working with conditions"){
			steps {
				script {
					a=100;b=20
					if ( a >= b )
					{
					println "Here your A varibale value is big: ${a}"					
					}
					else
					{
					println "Here is your B varibale value is big: ${b}"
					}
				     }
				}
			}
		}
	}
